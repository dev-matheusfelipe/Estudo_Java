public class variaveis {
  public static void main(String[] args) {
  
  // String - textos ex: "café", "escola", "Matheus Felipe"
  // Byte - números ex: 1,3,20 de -128 até 127
  // Short - números ex:1, 180, 900 de -32768 até 32767
  // Int - números ex: 1, 180 de -2147483648 até 2147483647
  // Long - números ex: 1, 500000 de -9223372036854775808 até 9223372036854775807
  // Float - números decimais ex: 1.2535 / precisão de 7
  // Double - números decimais ex: 1.2535 / precisão de 15 / 16
  // Boolean - valores booleanos - true / false
  
  string minhaVariavelString  = "café";
  System.out.println( minhaVariavelString );
  
  byte minhaVariavelByte = 20;
  System.out.println( minhaVariavelByte );
  
  short minhaVariavelShort = 3200;
  System.out.println( minhaVariavelShort );
  
  int minhaVariavelInt = 380000;
  System.out.println( minhaVariavelInt );
  
  long minhaVariavelLong = 10000000000L;
  System.out.println( minhaVariavelLong );
  
  float minhaVariavelFloat = 2.1234567f;
  System.out.println( minhaVariavelFloat );
  
  double minhaVariavelDouble = 2.12345678911234567;
  System.out.println( minhaVariavelDouble );
  
  double minhaVariavelDouble = 2.12345678912345678;
  System.out.println( minhaVariavelDouble );
  
  boolean minhaVariavelBoolean = true;
  System.out.println( minhaVariavelBoolean );
  
  boolean minhaVariavelBoolean = false;
  System.out.println( minhaVariavelBoolean );
  
  }
}
